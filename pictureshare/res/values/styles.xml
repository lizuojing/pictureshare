<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="font_share_title">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/font_share_title</item>
    </style>

    <style name="font_reg_btn">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ProgressBar_Horizontal_Web">
